{
  "name": "build-i18n",
  "version": "0.0.1",
  "description": "A simple script to parse Google Sheets into i18n json-files",
  "main": "./lib/index.js",
  "bin": {
    "ghseet-i18n-build": "./lib/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node src/index.ts -s 1ODbsOmAkvr9exvRNWehPvuQpD_cdaCLsLHMkzZKWb68'",
    "start:windows": "nodemon --watch 'src/**/*.ts' --exec \"npx ts-node\" src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "local": "sudo npm i -g && gsheet-i18n-build",
    "test": "echo \"test\"",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "keywords": [
    "i18n",
    "typescript",
    "node"
  ],
  "author": "Turbine Kreuzberg GmbH",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "nodemon": "^2.0.20",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "chalk": "^5.0.0",
    "clear": "^0.1.0",
    "commander": "^9.4.1",
    "figlet": "^1.5.2",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "public-google-sheets-parser": "^1.2.6"
  }
}
